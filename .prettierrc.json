{
  "semi": true,
  "trailingComma": "all",
  "singleQuote": true,
  "printWidth": 120,
  "tabWidth": 4,
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "endOfLine": "auto",
  "htmlWhitespaceSensitivity": "css",
  "cssDeclarationSortOrder": "alphabetical",
  "tableContentIndentation": "align",
  "vueIndentScriptAndStyle": true,
  "proseWrap": "preserve"
}
