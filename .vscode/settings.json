{
    "[typescriptreact]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
   "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "always"
    },
    "cSpell.words": [
        "ahooks",
        "antd",
        "bilkong",
        "craco",
        "echarts"
    ]
} 